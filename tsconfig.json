{
  "extends": "@sapphire/ts-config",
  "compilerOptions": {
    "allowJs": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "rootDir": "src",
    "outDir": "dist",
    "tsBuildInfoFile": "dist/.tsbuildinfo",
    "suppressImplicitAnyIndexErrors": true,
    "jsx": "react",
    "ignoreDeprecations": "5.0"
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
